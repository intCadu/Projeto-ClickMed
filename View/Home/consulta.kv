<consulta>:
    MDBoxLayout:
        orientation: "vertical"
        
        md_bg_color: 'lightblue'

        MDCard:
            id: Consulta
            orientation: 'vertical'
            size_hint: .85, .4
            pos_hint: {'center_x': .5,'center_y':.70}
            radius: 16  
            md_bg_color: 'lightblue'
            
            MDFloatLayout:
                Image:
                    source: 'Images/slogan.png'
                    resizable: True
                    allow_stretch: True
                    size_hint: .35, .35
                    pos_hint: {'center_x': .5,'center_y': .5}

                MDFillRoundFlatIconButton:
                    pos_hint: {'center_x': .25,'center_y': .15}    
                    theme_text_color: 'Custom'
                    specific_text_color: app.theme_cls.primary_color
                    allow_stretch: True
                    text_color: 'black'
                    text: 'Home'
                    icon: 'home'
                    icon_color: 'black'
                    md_bg_color: '#55C4BE'
                    elevation_normal: 8
                    size_hint_x: .3
                    on_release: 
                        root.goHome()
                        root.manager.transition.direction = 'right'

                MDFillRoundFlatIconButton:
                    pos_hint: {'center_x': .75,'center_y': .15}    
                    theme_text_color: 'Custom'
                    specific_text_color: app.theme_cls.primary_color
                    allow_stretch: True
                    text_color: 'black'
                    text: 'Diagnóstico'
                    icon: 'hospital-box'
                    icon_color: 'black'
                    md_bg_color: '#55C4BE'
                    elevation_normal: 8
                    size_hint_x: .3
                    on_release: 
                        root.goDiag()
                        root.manager.transition.direction = 'left'

        ScrollView:

            MDGridLayout:
                padding: dp(10)
                spacing: dp(10)
                cols: 1
                adaptive_height: True

                MDFillRoundFlatButton: 
                    text: 'Selecione os sintomas'
                    theme_text_color: 'Custom'
                    text_color: 'black'
                    size_hint_x: .3
                    md_bg_color: '#55C4BE'

                MDSeparator:

                MDStackLayout:
                    adaptive_height: True
                    spacing: dp(10)

                    MDChip:
                        id: DorCabeca
                        text: 'Dor de cabeça' 
                        text_color: 'black'
                        icon_right: 'pill-multiple'
                        md_bg_color: '#55C4BE'
                        active: False
                        on_active: root.adicionarSintoma()
                        allow_stretch: True
                        resizable: True

                    MDChip:
                        id: DorGarganta
                        text: 'Dor de garganta'
                        text_color: 'black'
                        icon_right: 'pill-multiple'
                        md_bg_color: '#55C4BE'
